{
	"name": "codex",
	"displayName": "codex",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codex.helloWorld",
		"onCommand:extension.nLToCode",
		"onCommand:extension.explainCode"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.nLToCode",
				"title": "Natural Language to Code"
			},
			{
				"command": "extension.explainCode",
				"title": "Explain code"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.nLToCode",
					"group": "YourGroup@1"
				},
				{
					"command": "extension.explainCode",
					"group": "YourGroup@1"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"axios": "^0.21.4"
	}
}
